""" Computes the sum of x numbers """

x = int(program.args[1])
result = 0
temp = 0
counter = 0

@for_range(x)
    temp = input_from(counter)
    if counter != x:
        print_str('%s + ', temp.reveal())
    else:
        print_str('%s = ', temp.reveal())
    result += temp
    counter += 1

print_str('%s\n', result)
