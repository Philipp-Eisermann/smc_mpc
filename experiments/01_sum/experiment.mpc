""" Computes the sum of x numbers """

x = int(program.args[1])
result = 0
array = sint.Array(1)
temp = 0

@for_range(x)
def _(i):
    array.input_from(i)
    temp += array[0]
    ''' Nicht secret values revealen
    if i != x:
        print_str('%s + ', temp.reveal())
    else:
        print_str('%s = ', temp.reveal())
    '''
    result += temp

print_str('%s\n', result)
